{
  "include": ["src/*.ts"],
  "compilerOptions": {
    "lib": ["ES2023", "DOM"],
    "target": "ES2022",
    "module": "Node16",
    "moduleResolution": "Node16",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "composite": true,
    "tsBuildInfoFile": "./dist/tsconfig.tsbuildinfo",
    "baseUrl": ".",
    "rootDir": "./src",
    "outDir": "./dist",
    "noEmit": false,
    "types": ["node"]
  }
}
