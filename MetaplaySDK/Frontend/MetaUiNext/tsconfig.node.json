{
  "include": [
    "vite.config.ts",
    ".storybook/**/*.ts",
    "tests/**/*.spec.ts",
    "src/unstable/timeline/timelineData*.ts",
    "src/unstable/timeline/*Types.ts",
    "src/unstable/timeline/*TimelineUtils.ts",
    "src/unstable/timeline/MEventTimelineColorUtils.ts",
    "src/composables/**/*.ts",
  ],
  "compilerOptions": {
    "lib": ["ES2023"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "composite": true,
    "noEmit": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "types": ["node"]
  }
}
